#!/bin/sh

# This normalization will also run check and fix routines
psfnormalize -p checkfix=fix source/masters/KayPhoDuMaster-Regular.ufo
psfnormalize -p checkfix=fix source/masters/KayPhoDuMaster-Bold.ufo

# Following sync will copy psnames and glyphorder updates to all fonts
psfsyncmasters source/KayPhoDu.designspace
